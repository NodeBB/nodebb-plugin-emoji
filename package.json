{
  "name": "nodebb-plugin-emoji",
  "version": "2.0.4",
  "description": "Adds extensible emoji functionality to NodeBB",
  "main": "build/lib",
  "author": "Peter Jaszkowiak <p.jaszkow@gmail.com> (https://github.com/pitaj)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/NodeBB/nodebb-plugin-emoji.git"
  },
  "nbbpm": {
    "compatibility": "^1.7.0"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "emoji",
    "extended"
  ],
  "dependencies": {
    "async": "^2.5.0",
    "fs-extra": "^4.0.1",
    "fuse.js": "^3.1.0",
    "lodash": "^4.17.4",
    "multer": "^1.3.0",
    "preact": "^8.2.5",
    "string-hash": "^1.1.3"
  },
  "devDependencies": {
    "@types/async": "^2.0.42",
    "@types/bootstrap": "^3.3.36",
    "@types/express": "^4.0.37",
    "@types/fs-extra": "^4.0.2",
    "@types/fuse": "^2.5.2",
    "@types/lodash": "^4.14.74",
    "@types/multer": "^1.3.3",
    "@types/nconf": "^0.0.34",
    "@types/node": "^8.0.26",
    "@types/socket.io": "^1.4.30",
    "tslint": "^5.7.0",
    "tslint-config-airbnb": "^5.2.1",
    "typescript": "^2.5.2"
  },
  "scripts": {
    "compile": "tsc -p . && tsc -p public",
    "prepare": "rm -r build && npm run compile && mkdir -p build/emoji && touch build/emoji/avoid_npm_ignore"
  }
}
